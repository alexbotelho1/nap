  <component id="ID_title1">
  <text><![CDATA[<b>Atribuir uma porta de switch </b>]]></text> 
  </component>

  <component id="ID_txt01">
  <text><![CDATA[<b>F0/1</b>]]></text> 
  </component>

  <component id="ID_txt02">
  <text><![CDATA[<b>F0/18</b>]]></text> 
  </component>

  <component id="ID_txt03">
  <text><![CDATA[<b>F0/1</b>]]></text> 
  </component>
  
  <component id="ID_box">
  <text><![CDATA[<b>Switch S1:<br>Porta F0/18<br>VLAN 20</b>]]></text> 
  </component>

  <component id="ID_box2">
  <text><![CDATA[<font face='_typewriter'>S1#<b>configure terminal</b><br>S1(config)#<b>interface F0/18</b><br>S1(config-if)#<b>switchport mode access</b><br>S1(config-if)#<b>switchport access vlan 20</b><br>S1(config-if)#<b>end</b></font>]]></text> 
  </component>

  <component id="ID_box3">
  <text><![CDATA[<b>PC do aluno<br>172.17.20.22</b>]]></text> 
  </component>

  <component id="ID_CS">
