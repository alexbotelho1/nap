      <trans-unit id="010" resname="IDS_TXT_04">
        <source>Check that the data cable is the correct cable and installed correctly.</source>
      </trans-unit>
      <trans-unit id="011" resname="IDS_TXT_04A">
        <source>Make sure that the printer is not plugged into a UPS.</source>
      </trans-unit>
      <trans-unit id="012" resname="IDS_TXT_04B">
        <source>Check that the glass surfaces are not dirty 
on the printer or scanner.</source>
      </trans-unit>
      <trans-unit id="013" resname="IDS_TXT_04C">
        <source>Make sure that the toner cartridge is full.</source>
      </trans-unit>
      <trans-unit id="014" resname="IDS_TXT_04D">
        <source>Check the age of the toner cartridge. </source>
      </trans-unit>
      <trans-unit id="015" resname="IDS_TXT_04E">
